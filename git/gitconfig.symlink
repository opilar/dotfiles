[user]
	name = opilarium
	email = opilar@ya.ru
	signingkey = opilar@ya.ru
[push]
	default = current
[core]
	quotepath = off
	editor = emacs
	excludesfile = /Users/opilar/.gitignore_global
	excludefile = /Users/opilar/.gitignore_global
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p

