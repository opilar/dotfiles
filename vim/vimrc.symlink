""" General

set nocompatible
filetype off

set autoread
let mapleader = ","

""" Plugins

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'
Plugin 'wombat256.vim'

call vundle#end()
filetype plugin indent on

""" UI

set t_Co=256
:colorscheme wombat256mod

set so=3

set wildmenu

syntax enable

""" Text

set expandtab
set smarttab
set wrap
